{
  "extends": [
    "next/core-web-vitals"
  ],
  "root": true,
  "rules": {
    "react/prop-types": "off",
    "react/react-in-jsx-scope": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "prettier/prettier": {
      "error": {
        "endOfLine": "auto"
      }
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "ignorePatterns": [
    "public/~partytown/*",
    "next-env.d.ts"
  ]
}
